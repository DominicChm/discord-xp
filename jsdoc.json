{
  "source": {
    "include": ["src/structures", "src/utils"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)|providers"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLins": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "tutorials": "./tutorials"
  }
}